{
    "name": "lcarr/football-api-sdk",
    "description": "software development kit for a football api",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Lcarr\\FootballApiSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcarr\\FootballApiSdk\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.5",
        "react/react": "^1.3",
        "emarref/jwt": "^1.0",
        "nativephp/electron": "^0.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3.2",
        "pestphp/pest": "^2.16",
        "pestphp/pest-plugin-drift": "^2.3"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
